
<%- include("partials/header")%>
<div class="container">
        <div id="home" class="jumbotron">
        <h1 id="good" class="text-center">List Of New Students</h1>
        <a class="btn btn-success btn-lg"href="/students/new">Add a new admission</a>
        </div>
        <div id="tok" class="row">
        <% for(var i=0 ; i < thisStudents.length ; i++){ %>
                <div class="col-lg-3 col-md-4 col-sm-6">
                        <div id="success"class="thumbnail">
                           <h2 id="good"><%=thisStudents[i].name %></h2>
                               <div id="caption" class="caption">
                                  <h4><%=thisStudents[i].branch%></h4>
        <!-- <h5><%=thisStudents[i].Rank%></h5> -->
                                      </div>
        <a id="caption" class="btn btn-success" href="/students/<%=thisStudents[i]._id%>">Read More</a>
        </div>
        </div>
        <% } %>
          </div>
         </div>
