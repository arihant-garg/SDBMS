<%- include("partials/header")%>
<div id="newcon"class="container">
    <div id="newter"class="row">
        <div class="col-12">
            <h1 class="text-center">EDIT STUDENT</h1>
        <form action="/students/<%=student._id%>?_method=PUT" method="POST">
            <div class="form-label-group">
            <label class="form-label" for="name">Name</label>
            <input class="form-control" type="text" name="student[name]" id="studentName" value="<%=student.name%>" required="required">
            </div class="form-label-group">
            <div>
                <label class="form-label"for="branch">Branch</label>
                <input class="form-control"type="text" name="student[branch]" id="StudentBranch"value="<%=student.branch%>" required="required">
                </div>
                <div class="form-label-group">
                    <label class="form-label"for="rank">Rank</label>
                    <input class="form-control"type="number" name="student[Rank]" id="StudentRank" value="<%=student.Rank%>" required="required">
                    </div>
                <button id="new" class="btn btn-success">SUBMIT</button>
        </form>
    </div>
</div>
</div>
