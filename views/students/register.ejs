<%- include("partials/header")%>
<div id="register" class="container">
<form class="form-signin" action="/register" method="POST">
    <h1 class="h3 mb-3 font-weight-normal text-center">Admin Sign In</h1>
    <div class="mb-3">
    <label for="inputEmail" class="sr-only">Username</label>
    <input type="text" id="inputEmail" class="form-control" name="username" placeholder="Username" required autofocus></div>
    <div class="mb-3">
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" name="password"class="form-control" placeholder="Password" required></div>
    <div class="mb-3">
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button></div>
    <div class="mb-3 text-center">
        <a href="/login">Already have an account, Login</a></div>
    <p class="mt-5 mb-3 text-muted text-center">&copy; Reaggaton</p>
  </form>
</div>


<%- include("partials/footer")%>