<%- include("partials/header")%>
<div id="register" class="container">
<form class="form-signin" action="/login" method="POST">
    <h1 class="h3 mb-3 font-weight-normal text-center">Login</h1>
    <div class="mb-3">
    <label for="inputEmail" class="sr-only">Username</label>
    <input type="text" id="inputEmail" class="form-control" name="username" placeholder="Username" required autofocus></div>
    <div class="mb-3">
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" class="form-control" name="password" placeholder="Password" required></div>
    <div class="mb-3">
    <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button></div>
    <p class="mt-5 mb-3 text-muted text-center">&copy; Reaggaton</p>
  </form>
  </div>
<%- include("partials/footer")%>