<%- include("partials/header")%>
        <div class="container">
                <div id="home" class="jumbotron">
                <h1 id="good" class="text-center">List Of New Lecturers</h1>
                <a class="btn btn-success btn-lg"href="/lecturers/new">Add a new Faculty</a>
                </div>
                <div id="tok" class="row">
                <% for(var i=0 ; i < thislecturers.length ; i++){ %>
                        <div class="col-lg-3 col-md-4 col-sm-6">
                                <div id="success"class="thumbnail">
                                   <h2 id="good"><%=thislecturers[i].name %></h2>
                                       <div id="caption" class="caption">
                                          <h4><%=thislecturers[i].branch%></h4>
                                              </div>
                <a id="caption" class="btn btn-success" href="/lecturers/<%=thislecturers[i]._id%>">Read More</a>
                </div>
                </div>
                <% } %>
                  </div>
                 </div>
