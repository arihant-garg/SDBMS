<%- include("partials/header")%>
<div id="newcon"class="container">
    <div id="newter"class="row">
        <div class="col-12">
            <h1 class="text-center">EDIT FACULTY</h1>
        <form action="/lecturers/<%=lecturer._id%>?_method=PUT" method="POST">
            <div class="form-label-group">
            <label class="form-label" for="name">Name</label>
            <input class="form-control" type="text" name="lecturer[name]" id="lecturerName" value="<%=lecturer.name%>" required="required">
            </div class="form-label-group">
            <div>
                <label class="form-label"for="qualification">Qualification</label>
                <input class="form-control"type="text" name="lecturer[qualification]" id="lecturerQualification"value="<%=lecturer.qualification%>" required="required">
                </div>
                <div class="form-label-group">
                    <label class="form-label"for="branch">Branch</label>
                    <input class="form-control"type="text" name="lecturer[branch]" id="lecturerBranch" value="<%=lecturer.branch%>" required="required">
                    </div>
                    <div class="form-label-group">
                        <label class="form-label"for="teachingExperience">Teaching Experience</label>
                        <input class="form-control"type="text" name="lecturer[teachingExperience]" id="lecturerTeachingExperience" value="<%=lecturer.teachingExperience%>" required="required">
                        </div>
                <button id="new" class="btn btn-success">SUBMIT</button>
        </form>
    </div>
</div>
</div>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
